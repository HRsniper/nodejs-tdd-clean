{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["eslint:recommended", "plugin:jest/recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["jest", "prettier"],
  "rules": { "comma-dangle": ["warn", "never"] },
  "settings": {
    "jest": {
      "version": 26
    }
  }
}
